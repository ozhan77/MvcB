@using ClassLibrary1.Concrete
@model List<Heading>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<h2>Index</h2>

<br/>
<table class="table table-bordered">
    <tr>
    <th>ID</th>
    <th>Headline</th>
    <th>Category</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.HeadingID</td>
            <td>@item.HeadingName</td>
            <td>@item.CategoryID</td>
        </tr>
    }
</table>
<br/>
<a href="/Heading/AddHeading" class="btn btn-secondary">New Heading</a>